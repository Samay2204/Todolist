<%- include('header'); -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
        
            <!-- <li> <%= listitems %> </li> -->
             <% for(var i=0; i<listitems.length; i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p> <%= listitems[i] %> </p>
                </div>
                
                <% } %>

                <form action="/" method="post" class="item">
                    <input type="text" name="newlistitem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="btn"  value=<%= listTitle  %> >+</button>
                </form>
        
    </div>
    

<%- include('footer'); -%>
   

  